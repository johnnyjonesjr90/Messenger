{
    <div float="left">
        <img src="@Model.ImageDataUrl" style="width:100px; height:100px; border-radius: 50%; overflow: hidden; position: relative " />
    </div>
}
else
{
    <img src="~/no-profile-pic-icon-24.jpg" style="width: 30%;border-radius:50%" />
}
